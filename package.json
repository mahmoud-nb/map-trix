{
  "name": "map-trix",
  "version": "1.4.2",
  "description": "This module provides an easy to use interface that allows to load and use Google Maps API",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "author": {
    "name": "Mahmoud NBET",
    "email": "mahmoud.nb@gmail.com"
  },
  "keywords": [
    "google",
    "maps",
    "loader",
    "interface",
    "typescript"
  ],
  "homepage": "https://github.com/mahmoud-nb/map-trix",
  "repository": {
    "type": "git",
    "url": "git@github.com:mahmoud-nb/map-trix.git"
  },
  "license": "ISC",
  "scripts": {
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "tsc": "tsc",
    "build": "tsc && rollup -c rollup.config.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/google.maps": "^3.52.6",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.0.4"
  }
}
